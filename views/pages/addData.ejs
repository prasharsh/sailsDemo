    <input
      type="text"
      ,
      name="jobName"
      ,
      placeholder="Enter Job Name"
      ,
      required
    />
    <input
      type="number"
      ,
      name="partID"
      ,
      placeholder="Enter Part ID"
      ,
      required
    />
